void spawnEnemy() {
 for (int i = 0; i < 5; i++) {
  if (!enemies[i].active && enemies[i].erased) {
   enemies[i].active = 1;
   enemies[i].erased = 0;
   enemies[i].rdel = 0;
   enemies[i].slowframe = 0;
   enemies[i].row = 10 + rand() % (160 - 10 - enemies[i].height);

   int choice = rand() % 2;
   if (choice == 0) {

    enemies[i].col = 0;
    enemies[i].cdel = 1;
   } else if (choice == 1) {

    enemies[i].col = 240 - enemies[i].width;
    enemies[i].cdel = -1;
   }
   enemies[i].oldCol = enemies[i].col;
   enemies[i].oldRow = enemies[i].row;

   spawnDelay = 50 + (rand() % 50);
   break;
  }
 }
}


void spawnSmallEnemies(ENEMY* parent) {

 for (int i = 0; i < 10; i++) {
  if (!smallEnemies[i].active && smallEnemies[i].erased) {
   smallEnemies[i].active = 1;
   smallEnemies[i].erased = 0;
   smallEnemies[i].rdel = 1;
   smallEnemies[i].cdel = parent->cdel;
   smallEnemies[i].slowframe = 0;
   smallEnemies[i].row = parent->row;
   smallEnemies[i].col = parent->col;
   smallEnemies[i].oldCol = smallEnemies[i].col;
   smallEnemies[i].oldRow = smallEnemies[i].row;
   smallEnemies[i].dir = UP;
   break;
  }
 }

 for (int i = 0; i < 10; i++) {
  if (!smallEnemies[i].active && smallEnemies[i].erased) {
   smallEnemies[i].active = 1;
   smallEnemies[i].erased = 0;
   smallEnemies[i].rdel = -1;
   smallEnemies[i].cdel = parent->cdel;
   smallEnemies[i].slowframe = 0;
   smallEnemies[i].row = parent->row;
   smallEnemies[i].col = parent->col;
   smallEnemies[i].oldCol = smallEnemies[i].col;
   smallEnemies[i].oldRow = smallEnemies[i].row;
   smallEnemies[i].dir = DOWN;
   break;
  }
 }
}


void updateShip() {
 Sprite* sprite = &ship.sprite;

 int hor = NEUTRAL;
 int ver = NEUTRAL;


 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))) {
  hor = LEFT;
 } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
  hor = RIGHT;
 }
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6)))) {
  ver = UP;
 } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7)))) {
  ver = DOWN;
 }


 if (hor == LEFT) {
  sprite->col -= sprite->cDel;
  if (sprite->col < 0) {
   sprite->col = 0;
  }
 } else if (hor == RIGHT) {
  sprite->col += sprite->cDel;
  if (sprite->col + sprite->width - 1 >= 240) {
   sprite->col = 240 - sprite->width;
  }
 }

 int oldOrient = ship.hOrient;
 if (hor != NEUTRAL) {

  ship.hOrient = hor;
 }
 if (ver == UP) {
  sprite->row -= sprite->rDel;
  if (sprite->row < 10) {
   sprite->row = 10;
  }
 } else if (ver == DOWN) {
  sprite->row += sprite->rDel;
  if (sprite->row + sprite->height - 1 >= 160) {
   sprite->row = 160 - sprite->height;
  }
 }


 if (oldOrient != ship.hOrient) {
  if (ship.hOrient == LEFT) {
   ship.sprite.image = submarine_left;
  } else {
   ship.sprite.image = submarine_right;
  }
 }


 if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && ship.cooldown <= 0) {
   fireTorpedo(hor, ver);

  ship.cooldown = 16;
 } else {
  ship.cooldown--;
 }

 if (ship.iframes > 0) {

  if (ship.iframes % 4 == 0) {

   ship.invis = ship.invis ? 0 : 1;
  }

  ship.iframes--;
 } else {

  ship.invis = 0;
 }
}


void fireTorpedo() {
 for (int i = 0; i < 6; i++) {
  if (!torpedoes[i].active) {
   torpedoes[i].active = 1;
   torpedoes[i].erased = 0;
   torpedoes[i].row = ship.sprite.row + 11;

   if (ship.hOrient == LEFT) {
    torpedoes[i].cdel = -3;
    torpedoes[i].col = ship.sprite.col - torpedoes[i].width;
   } else if (ship.hOrient == RIGHT) {
    torpedoes[i].cdel = 3;
    torpedoes[i].col = ship.sprite.col + ship.sprite.width - 1;
   }
   torpedoes[i].oldCol = torpedoes[i].col;
   torpedoes[i].oldRow = torpedoes[i].row;
   break;
  }
 }
}


void updateTorpedo(TORPEDO* b) {
 if (b->active) {
  b->col += b->cdel;
  if (b->col < 0 || b->col >= 240) {
   b->active = 0;
  }
 }
}


void updateEnemy(ENEMY* b, int small) {

 if (b->active) {



  if (!b->slowframe) {


   if (!small) {
    int tmp = rand() % 6;
    switch(tmp) {
    case 0:
     b->rdel = 1;
     break;
    case 1:
     b->rdel = -1;
     break;
    default:
     b->rdel = 0;
    }
   } else {
    int tmp = rand() % 6;
    if (b->dir == UP) {
     switch(tmp) {
     case 0:
      b->rdel = 2;
      break;
     case 1:
      b->rdel = 0;
      break;
     default:
      b->rdel = 1;
     }
    } else {
     switch(tmp) {
     case 0:
      b->rdel = -2;
      break;
     case 1:
      b->rdel = 0;
      break;
     default:
      b->rdel = -1;
     }
    }
   }

   b->row += b->rdel;
   b->col += b->cdel;
  }


  b->slowframe = b->slowframe ? 0 : 1;


  if (b->row < 10 || b->col < 0 || b->row > 160 - b->height || b->col > 240 - b->width) {
   b->active = 0;
   b->erased = 0;
  }


  for (int i = 0; i < 6; i++) {
   if (torpedoes[i].active) {
    if (collision(torpedoes[i].col, torpedoes[i].row, torpedoes[i].width, torpedoes[i].height, b->col, b->row, b->width, b->height)) {
     torpedoes[i].active = 0;
     b->active = 0;
     if (!small) {
      score += 500;

      spawnSmallEnemies(b);
     } else {
      score += 1000;
     }
    }
   }
  }


  if (collision(ship.sprite.col, ship.sprite.row, ship.sprite.width, ship.sprite.height, b->col, b->row, b->width, b->height)) {
   b->active = 0;

   if (ship.iframes <= 0) {
    lives--;
    ship.iframes = 90;
   }
  }
 }
}


void drawHUD() {



 drawRect(1 + 6 * 6, 1, 6, 8, 0);

 drawChar(1 + 6 * 6, 1, (char) ('0' + lives), ((31) | (31)<<5 | (31)<<10));

 drawRect(240 - 1 - 6 * 22, 1, 6 * 6, 8, 0);

 drawScore(1, 1, 22, score, ((31) | (31)<<5 | (31)<<10));
}


void drawShip() {

 if (!ship.invis) {
  eraseMovingSprite(&ship.sprite, bgColor);
  drawSprite(&ship.sprite);
 } else {
  eraseFullSprite(&ship.sprite, bgColor);
 }

 ship.sprite.oldRow = ship.sprite.row;
 ship.sprite.oldCol = ship.sprite.col;
}


void drawTorpedo(TORPEDO* b) {
 if(b->active) {
  drawRect(b->oldCol, b->oldRow, b->width, b->height, bgColor);
  drawRect(b->col, b->row, b->width, b->height, b->color);
 } else if (!b->erased) {
  drawRect(b->oldCol, b->oldRow, b->width, b->height, bgColor);
  b->erased = 1;
 }
 b->oldRow = b->row;
 b->oldCol = b->col;
}


void drawEnemy(ENEMY* b) {
 if(b->active) {

  if (b->slowframe) {
   eraseMovingImage(b->col, b->row, b->oldCol, b->oldRow, b->width, b->height, bgColor);
  }

  drawImage(b->col, b->row, b->width, b->height, b->image);
 } else if (!b->erased) {
  drawRect(b->oldCol, b->oldRow, b->width, b->height, bgColor);
  b->erased = 1;
 }
 b->oldRow = b->row;
 b->oldCol = b->col;
}
