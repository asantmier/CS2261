 if (timer <= 0) {
  score += 10;
  timer = 60;
 }
 timer--;


 if (spawnDelay <= 0) {
  spawnEnemy();
 } else {
  spawnDelay--;
 }

 updateShip();

 for (int i = 0; i < 6; i++) {
  updateTorpedo(&torpedoes[i]);
 }

 for (int i = 0; i < 5; i++) {
  updateEnemy(&enemies[i], 0);
 }

 for (int i = 0; i < 10; i++) {
  updateEnemy(&smallEnemies[i], 1);
 }
}


